#
# runtime configuration for QCaaS project
#

qcaas.env=prod

# whether messages are expected to be digitally signed and validated
qcaas.request.validate=false

# location of test scripts
# default location: <tomcat-installation-root>/content/qcaas/bin
# qcaas.qc.script.dir=content/qcaas/bin

# test-to-script mapping
# qcaas.qc.script.<test_name>=<script_name>

# script input mode (file | stdin)
# qcaas.qc.script.<test_name>.mode=<mode>
